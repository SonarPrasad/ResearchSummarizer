{
    "version": 2,
    "builds": [
      {
        "src": "ResearchSummarizer/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "1024mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "ResearchSummarizer/wsgi.py"
      }
    ]
  }